<script type="text/javascript">
class KLPWA{
    constructor(){
        this.init();
    }
    async init(){
        this.worker = {};
        this.registerWorker();
    }
    async postMessage(data){
        if(
            !this.worker
            || !this.worker.controller
            || typeof this.worker.controller.postMessage !== 'function'
        ){
            return false;
        };
        return new Promise(function(resolve, reject){
            const messageChannel = new MessageChannel();
            messageChannel.port1.onmessage = function(event){
                if(event.data.error){
                    reject(event.data.error);
                }else{
                    resolve(event.data);
                };
            };
            navigator.serviceWorker.controller.postMessage(data, [messageChannel.port2])
        });
    }
    async registerWorker(){
        if(
            typeof window.navigator !== 'object'
            || typeof window.navigator.serviceWorker !== 'object'
            || typeof window.navigator.serviceWorker.register !== 'function'
        ){
            window.console.warn('[ServiceWorker]','ServiceWorker unavailable!');
            return;
        };
        try{
            this.worker = window.navigator.serviceWorker;
            await this.worker.register('/serviceworker.js');
            this.postMessage({event:'pageloaded', data:window.location.href});
        }catch(error){
            window.console.warn('ServiceWorker registration failed!', error);
        }
    }
};
