<!-- Iframe Delay -->
<script type="text/javascript">
    window.addEventListener('load', function(){
        if(!$ || !$.fn){return;};
        $('.iframe-delay').each(function(index, element){
            var src = $(this).data('src');
            if(!src){return};
            var thisElm = $(this);
            
            var width = thisElm.data('width')||560;
            var height = thisElm.data('height')||315;
            var delay = thisElm.data('delay')||3000;
            var title = thisElm.data('title')||'Iframe';
            var type = thisElm.data('type')||'html';
            
            width = window.parseInt(width);
            height = window.parseInt(height);
            delay = window.parseInt(delay);
            
            var sendEvent = null;
            var eventData = {
                src: src,
                index: index,
                width: width,
                height: height,
                delay: delay,
                type: type,
                element: element
            };
            
            sendEvent = new CustomEvent('iframe-delay', {
                detail:{event: 'initialized', data: eventData}
            });
            window.dispatchEvent(sendEvent);
            
            var loadIframe = function(){
                var iframeElm = $('<iframe></iframe>');
                iframeElm.attr('frameborder', 0);
                iframeElm.attr('scrolling', 0);
                iframeElm.attr('width', width);
                iframeElm.attr('height', height);
                iframeElm.attr('src', src);
                iframeElm.attr('title', title);
                thisElm.addClass('iframe-loaded');
                
                iframeElm.on('load', function(){
                    sendEvent = new CustomEvent('iframe-delay', {
                        detail:{event: 'loaded', data: eventData}
                    });
                    window.dispatchEvent(sendEvent);
                });
                
                thisElm.html(iframeElm);
            };
            
            window.setTimeout(loadIframe, delay);
        });
    });
</script>
<!-- Endof Iframe Delay-->

