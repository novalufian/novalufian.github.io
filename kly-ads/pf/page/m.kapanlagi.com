<!-- Set KLY Visitor  -->
<script type="text/javascript">
window.addEventListener('load', function(){
    if(!window.kly){return;};
    
    window.kly.visitor = window.kly.visitor || {};
    window.kly.gtm = window.kly.gtm || {};
    var document = window.document;
    var userId = window.liputan6_id_u_id || '';
    var cookie = document.cookie || '';
    var userGender = window.liputan6_id_gender || '';
    var userAge = window.liputan6_id_age || '';
    
    var trackers = [];
    var clientId = '';
    
    var readVisitorCookie = function(cookieName){
        if(typeof cookie.match !== 'function'){return null;};
        var regexMatch = new RegExp('(^| )' + cookieName + '=([^;]+)');
        var cookieMatch = cookie.match(regexMatch);
        if(!cookieMatch || !cookieMatch[2]){
            return null;
        };
        if(cookieMatch[2] == 'null'){
            return null;
        };
        return cookieMatch[2];
    };
    
    if(
        typeof ga === 'function'
        && typeof ga.getAll === 'function'
    ){
        trackers = ga.getAll();
    };
    
    // Get it from GA-Api.
    for (var i = 0; i < trackers.length; i++){
        var tracker = trackers[i];
        if(typeof tracker !== 'object')continue;
        if(typeof tracker.get !== 'function')continue;
        
        // Get the last.
        trackerClientId = tracker.get('clientId');
        if(!trackerClientId || typeof trackerClientId !== 'string')
            continue;
        
        clientId = trackerClientId;
    }; 
    
    if(!clientId){
        clientId = readVisitorCookie('_ga');
        if(clientId){
            clientId = clientId.split('.').slice(-2).join(".");
        };
    };
    if(!userGender){userGender = readVisitorCookie('liputan6_id_gender');};
    if(!userAge){userAge = readVisitorCookie('liputan6_id_age');};
    if(!userId){userId = readVisitorCookie('liputan6_id_u_id');};
    if(!userId){userId = readVisitorCookie('liputan6_id_u_id_kapanlagi');};
    
    var login_status = 'not_logged_in';
    if(userId){login_status = 'logged_in';};
    
    // Add to window.kly.
    if(clientId){window.kly.visitor.gaid = clientId;};
    if(userGender){window.kly.visitor.gender = userGender;};
    if(userAge){window.kly.visitor.age = userAge;};
    if(login_status){
        window.kly.gtm.login_status = login_status;
        window.kly.visitor.login_status = login_status;
    };
});
</script>
<!-- Endof Set KLY Visitor -->
</body>
</html>
